{
    "name": "simplesamlphp/simplesamlphp-module-sanitycheck",
    "description": "Perform sanity checks on configuration",
    "type": "simplesamlphp-module",
    "keywords": ["simplesamlphp", "sanitycheck"],
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andreas Ã…kre Solberg",
            "email": "andreas.solberg@uninett.no"
        }
    ],
    "config": {
        "preferred-install": {
            "simplesamlphp/simplesamlphp": "source",
            "*": "dist"
        }
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\Module\\sanitycheck\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\Utils\\": "vendor/simplesamlphp/simplesamlphp/tests/Utils"
        }
    },
    "require": {
        "php": ">=7.2",
        "simplesamlphp/simplesamlphp": "dev-master",
        "simplesamlphp/composer-module-installer": "~1.1"
    },
    "require-dev": {
        "simplesamlphp/simplesamlphp-test-framework": "^0.1.3"
    },
    "support": {
        "issues": "https://github.com/simplesamlphp/simplesamlphp-module-sanitycheck/issues",
        "source": "https://github.com/simplesamlphp/simplesamlphp-module-sanitycheck"
    }
}
